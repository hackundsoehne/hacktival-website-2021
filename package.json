    "url": "https://github.com/hackundsoehne/hacktival-website-2021/issues"
  },
  "homepage": "https://github.com/hackundsoehne/hacktival-website-2021#readme",
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "browser-sync": "^2.26.14",
    "postcss": "^8.2.6",
    "prettier": "2.2.1",
    "tailwindcss": "^2.0.3"
  },
  "dependencies": {
    "tailwindcss-cli": "^0.1.2"
  }
